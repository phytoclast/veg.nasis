#cover = aggregate overstory cover
#diam = stand quadratic mean diameter
#ba = stand basal area count
#baf = basal area factor
if(is.na(baf)){baf=10}
if(baf %in% c(5,10,20,40)){#legacy basal area units
fba = baf*ba
actoha = 43560*0.3048^2/10000
